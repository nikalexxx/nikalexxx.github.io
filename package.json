{
  "name": "nikalexxx.github.io",
  "version": "1.21.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "patch-html": "VERSION=${npm_package_version} node html-gen.js",
    "build": "rm -Rf dist; parcel build index.html; npm run patch-html; npm run build-books",
    "build-books": "OUTPUT=dist node data/books/build.js",
    "dev-books": "onchange \"./data/books/**/*\" -- OUTPUT=dev-dist npm run build-books",
    "dev-blog": "onchange \"./data/blog/**/*\" -- npm run cp-dev-blog",
    "cp-dev-blog": "cp -R data/blog dev-dist/data",
    "deploy": "ENV=prod npm run build; cp -R data/blog dist/data; cp -R data/images dist/data",
    "dev": "rm -Rf dev-dist; mkdir dev-dist; cp -R data dev-dist/data; OUTPUT=dev-dist node data/books/build.js; INPUT=index.html OUTPUT=dev.html npm run patch-html; parcel -d dev-dist dev.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikalexxx/nikalexxx.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nikalexxx/nikalexxx.github.io/issues"
  },
  "homepage": "https://github.com/nikalexxx/nikalexxx.github.io#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@types/gh-pages": "^3.2.0",
    "@types/node": "^16.9.6",
    "gh-pages": "^3.2.0",
    "less": "^3.13.1",
    "onchange": "^7.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-wasm.rs": "^1.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "katex": "^0.12.0"
  }
}
